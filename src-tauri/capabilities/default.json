{
  "$schema": "https://schemas.tauri.app/capability/2.0.0-rc.schema.json",
  "identifier": "default",
  "description": "Default capabilities for the app",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:default",
    "fs:allow-read",
    "shell:default",
    "shell:allow-execute",
    "shell:allow-spawn"
  ]
}
